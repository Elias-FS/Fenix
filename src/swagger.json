                  },
                  "document": {
                    "type": "string"
                  },
                  "typeDocument": {
                    "type": "string",
                    "enum": [
                      "CNPJ"
                    ],
                    "description": "Default type document is CNPJ"
                  },
                  "slug": {
                    "type": "string"
                  },
                  "status": {
                    "type": "string",
                    "enum": [
                      "active",
                      "inactive"
                    ],
                    "description": "Default status is active"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Created",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "example": {
                    "title": "Forest Watch",
                    "document": "39317935000131",
                    "slug": "TECNOLOGIA",
                    "typeDocument": "CNPJ",
                    "status": "active"
                  }
                }
              }
            }
          },
          "500": {
            "description": "Organization with document already exists"
          },
          "400": {
            "description": "Invalid document (if cpf is specified, document must be 11 numbers or if cnpj is specified, document must be 14 numbers)"
